<h3>NEW REVIEW</h3>
<form {{action "submitReview" content on="submit"}}>
  <label>Rating</label>
  {{view  Ember.Select content=ratingChoices value=rating required=true prompt="Select Rating"}}
  {{textarea value=post required=true}}
  <button class="btn btn-primary btn-block" type="submit">Save</button>
</form>
